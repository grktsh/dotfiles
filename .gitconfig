[user]
	name = grktsh
	email = grktsh@gmail.com
[color]
	ui = auto
[alias]
	br = branch
	ci = commit
	co = checkout
	st = status
[core]
	excludesfile = ~/.gitignore_global
	preloadindex = true
[ghq]
	root = ~/src
	root = ~/.go/src
[filter "lfs"]
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
